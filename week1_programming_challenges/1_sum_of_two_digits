using System;

namespace APlusB
{
    class Program
    {
        static void Main(string[] args)
        {
            string[] m = Console.ReadLine().Split();
            int a = int.Parse(m[0]);
            int b = int.Parse(m[1]);
            int c = a + b;
            Console.Write(c);
            Console.Read();
        }
    }
}
